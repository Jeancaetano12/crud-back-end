datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         String   @id @default(uuid())
  name       String
  email      String   @unique
  telefone   String?
  birth_date DateTime? @map("birth_date")
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("users")
}